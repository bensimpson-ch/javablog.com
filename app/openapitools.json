{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "generators": {
      "v1": {
        "generatorName": "typescript-angular",
        "inputSpec": "../services/api/src/main/resources/META-INF/openapi-v1.yaml",
        "output": "./src/app/api/v1",
        "additionalProperties": {
          "ngVersion": "20.0.0",
          "providedInRoot": true,
          "useSingleRequestParameter": false,
          "withInterfaces": true,
          "stringEnums": true
        },
        "globalProperty": {
          "skipFormModel": "true"
        }
      }
    }
  }
}
